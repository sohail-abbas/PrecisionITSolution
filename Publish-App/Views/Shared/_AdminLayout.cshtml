<!DOCTYPE html>
<html>
<head>
    <!-- Title -->
    <title>@ViewBag.Title</title>
    <!-- Meta -->
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <!-- Favicon -->
    @*<link href="favicon.ico" rel="shortcut icon" />*@
    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="~/Content/theme-3/css/bootstrap.css" />
    <!-- Template CSS -->
    <link rel="stylesheet" href="~/Content/theme-3/css/animate.css" />
    <link rel="stylesheet" href="~/Content/theme-3/css/font-awesome.css" />
    <link rel="stylesheet" href="~/Content/theme-3/css/nexus.css" />
    <link rel="stylesheet" href="~/Content/theme-3/css/responsive.css" />
    <link rel="stylesheet" href="~/Content/theme-3/css/custom.css" />
    <!-- Google Fonts-->
    <link href="http://fonts.googleapis.com/css?family=Roboto+Condensed:400,300" rel="stylesheet" type="text/css">
    <link href="~/Content/css/jquery.dataTables.min.css" rel="stylesheet" />
</head>
<body>
    <div id="body-bg">
        <!-- Phone/Email -->
        <div id="pre-header" class="background-gray-lighter">
            <div class="container no-padding">
                <div class="row hidden-xs">
                    <div class="col-sm-4 padding-vert-5">
                        <strong class="fa-phone">Phone:</strong>&nbsp;+91-9811557969; +91-9811961201
                    </div>
                    <div class="col-sm-4 text-center padding-vert-5">
                        {Put Company Name Here}
                    </div>
                    <div class="col-sm-4 text-right padding-vert-5">
                        <strong class="fa-envelope">Email:</strong>&nbsp;info@precisionitsolution.com
                    </div>
                </div>
            </div>
        </div>
        <!-- End Phone/Email -->
        @if (ViewBag.ActiveClass == "home")
        {
            <div id="header">
                <div class="container">
                    <div class="row">
                        <!-- Logo -->
                        <div class="logo pull-left">
                            <a href="~/Home/Index" title="">
                                <img src="~/Content/theme-3/img/logo.png" alt="Logo" />
                            </a>
                        </div>
                        <!-- End Logo -->
                    </div>
                </div>
            </div>
        }
        <!-- Top Menu -->
        <div id="hornav" class="bottom-border-shadow">
            <div class="container no-padding border-bottom">
                <div class="row">
                    <div class="col-md-12 no-padding">
                        <div class="visible-lg">
                            <ul id="hornavmenu" class="nav navbar-nav">
                                @Html.Partial("~/Views/Shared/_MenuWithoutLogin.cshtml")
                                @Html.Partial("~/Views/Shared/_MenuWithLogin.cshtml")
                            </ul>
                        </div>
                    </div>
                    @*<div class="col-md-4 no-padding">
                            <ul class="social-icons pull-right">
                                <li class="social-rss">
                                    <a href="#" target="_blank" title="RSS"></a>
                                </li>
                                <li class="social-twitter">
                                    <a href="#" target="_blank" title="Twitter"></a>
                                </li>
                                <li class="social-facebook">
                                    <a href="#" target="_blank" title="Facebook"></a>
                                </li>
                                <li class="social-googleplus">
                                    <a href="#" target="_blank" title="Google+"></a>
                                </li>
                            </ul>
                        </div>*@
                </div>
            </div>
        </div>
        <!-- End Top Menu -->
        <!-- === END HEADER === -->
        @RenderBody()
        <div id="loadingModal" style="display:none">
            <div>Loading.............</div>
        </div>
        <!-- === BEGIN FOOTER === -->
        <div id="base">
            <div class="container bottom-border padding-vert-30">
                <div class="row">
                    <!-- Disclaimer -->
                    <div class="col-md-4">
                        <h3 class="class margin-bottom-10">Disclaimer</h3>
                        <p class="text-justify">All images on this domain are for presentation purposes only, intended to represent the service, and offering by Precision IT Solution.</p>
                        <p class="text-justify">
                            Most of the images used here are from
                            <a href="http://www.google.com/" target="_blank">google.com</a>. Links are provided if you wish to purchase them from their copyright owners.
                        </p>
                    </div>
                    <!-- End Disclaimer -->
                    <!-- Contact Details -->
                    <div class="col-md-4 margin-bottom-20">
                        <h3 class="margin-bottom-10">Contact Details</h3>
                        <p>
                            <span class="fa-phone">Telephone:</span>+91-9811557969<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; +91-9811961201
                            <br>
                            <span class="fa-envelope">Email:</span>
                            <a href="mailto:info@example.com">info@precisionitsolution.com</a>
                            <br>
                            <span class="fa-link">Website:</span>
                            <a href="http://www.example.com">www.precisionitsolution.com</a>
                        </p>
                        <p>
                            <span class="fa-map-marker">Adress:</span> D-13/1, First Floor,
                            <br>A. F. Enclave - II, Jamia Nagar
                            <br>Okhla, New Delhi - 110025
                            <br>India
                        </p>
                    </div>
                    <!-- End Contact Details -->
                    <!-- Sample Menu -->
                    <div class="col-md-4 margin-bottom-20">
                        <h3 class="margin-bottom-10">Quick Link</h3>
                        <ul class="menu">
                            <li>
                                <a class="fa-tasks" href="~/Home/About">Know about Us</a>
                            </li>
                            <li>
                                <a class="fa-users" href="~/Home/Career">Work with Us</a>
                            </li>
                            <li>
                                <a class="fa-signal" href="~/Home/Service">Service Info</a>
                            </li>
                            <li>
                                <a class="fa-coffee" href="~/Appointment/AddRequest">Book an Appointment</a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <!-- End Sample Menu -->
                </div>
            </div>
        </div>
        <!-- Footer -->
        <div id="footer" class="background-grey">
            <div class="container">
                <div class="row">
                    <!-- Footer Menu -->
                    <div id="footermenu" class="col-md-8">
                        <ul class="list-unstyled list-inline">
                            <li>
                                <a href="#" target="_blank">Sample Link</a>
                            </li>
                            <li>
                                <a href="#" target="_blank">Sample Link</a>
                            </li>
                            <li>
                                <a href="#" target="_blank">Sample Link</a>
                            </li>
                            <li>
                                <a href="#" target="_blank">Sample Link</a>
                            </li>
                        </ul>
                    </div>
                    <!-- End Footer Menu -->
                    <!-- Copyright -->
                    <div id="copyright" class="col-md-4">
                        <p class="pull-right">(c) <script>
                            document.write(new Date().getFullYear())</script> Precision IT Solution </p>
                    </div>
                    <!-- End Copyright -->
                </div>
            </div>
        </div>
        <script type="text/javascript" src="~/Content/theme-3/js/jquery.min.js"></script>
        <script type="text/javascript" src="~/Content/theme-3/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="~/Content/theme-3/js/scripts.js"></script>
        <!-- Isotope - Portfolio Sorting -->
        <script type="text/javascript" src="~/Content/theme-3/js/jquery.isotope.js"></script>
        <!-- Mobile Menu - Slicknav -->
        <script type="text/javascript" src="~/Content/theme-3/js/jquery.slicknav.js"></script>
        <!-- Animate on Scroll-->
        <script type="text/javascript" src="~/Content/theme-3/js/jquery.visible.js" charset="utf-8"></script>
        <!-- Sticky Div -->
        <script type="text/javascript" src="~/Content/theme-3/js/jquery.sticky.js" charset="utf-8"></script>
        <!-- Slimbox2-->
        <script type="text/javascript" src="~/Content/theme-3/js/slimbox2.js" charset="utf-8"></script>
        <!-- Modernizr -->
        <script src="~/Content/theme-3/js/modernizr.custom.js"></script>
        <script src="~/Content/js/jquery.validate.js"></script>
        <script src="~/Content/js/application-master.js"></script>
        @RenderSection("scripts", required: false)
</body>
</html>
